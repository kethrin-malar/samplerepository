package com.insurance.demo.business.impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.insurance.demo.business.DriverBusiness;
import com.insurance.demo.entity.DriverEntity;
import com.insurance.demo.service.DriverService;


@Service
public class DriverBusinessImpl implements DriverBusiness {

    @Autowired
    private DriverService driverService;

    public String createDriver(DriverEntity driver) {
        // Add any additional business logic here if needed
        String msg = driverService.createDriver(driver);
        return msg;
    }
}
